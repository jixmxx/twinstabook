<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="postns">
<select id="list" parameterType="hashMap" resultType="post">
	select * from
		(select * from post
		order by crt_date)
	where rownum &gt;= 1
		and rownum &lt;= 10
</select>
</mapper> 